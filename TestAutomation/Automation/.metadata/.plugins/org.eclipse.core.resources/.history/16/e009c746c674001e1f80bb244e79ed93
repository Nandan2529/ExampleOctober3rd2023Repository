package www.sgtesting.test1;

class Array
{
	int[] Array(int a[])
	{
		int x[]=new int[a.length];
		int k=0;
		for (int i=0;i<a.length;i++)
		{
			x[k]=a[i];
			k++;
		}
		return x;
	}
    
}
public class Test9 {
	public static void main(String[] args) {
		Array o=new Array();
		int c[]= {10,20,30,40,50,60};
		int b[]=o.Array(c);
		for (int i=0;i<b.length/2;i++)
		{
			System.out.println(b[i]);
		}

	}

}
