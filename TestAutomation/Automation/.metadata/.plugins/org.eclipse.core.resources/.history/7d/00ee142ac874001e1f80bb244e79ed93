package www.sgtesting.test1;

class Demoa
{
	int[] Array(int x[][])
	{
		int a[]=new int[x.length*x[0].length];
		int k=0;
		for (int i=0;i<x.length;i++)
		{
			for (int j=0;j<x[i].length;j++)
			{
				a[k]=x[i][j];
				k++;
			}
		}
		return a;
	}
}
public class TwoD {
	public static void main(String[] args) {
		Demoa o=new Demoa();
		int y[][]= {{10,15,20},{25,30,35}};
		System.out.println(y);

	}

}
